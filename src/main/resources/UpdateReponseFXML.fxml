<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane prefHeight="297.0" prefWidth="448.0" stylesheets="@dashboardDesign.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.controllers.UpdateReponseController">
   <content>
      <VBox prefHeight="286.0" prefWidth="442.0">
         <children>
            <VBox fx:id="vboxEtat">
               <children>
                  <TextField fx:id="tfetat" promptText="etat" />
               </children>
            </VBox>
            <VBox fx:id="vboxContenu">
               <children>
                  <TextArea fx:id="tacontenu" prefHeight="200.0" prefWidth="200.0" promptText="message" />
               </children>
            </VBox>
            <Button mnemonicParsing="false" onAction="#updateReponse" text="save" />
         </children>
      </VBox>
   </content>
   <styleClass>
      <String fx:value="white-bg" />
      <String fx:value="shadow" />
   </styleClass>
</ScrollPane>
